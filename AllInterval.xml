<instance format="XCSP3" type="CSP">
    <variables>
        <array id="x" note="x[i] is the ith value of the series" size="[6]"> 0..5 </array>
        <array id="y" note="y[i] is the distance between x[i] and x[i+1]" size="[5]"> 1..5 </array>
    </variables>
    <constraints>
        <allDifferent> x[] </allDifferent>
        <allDifferent> y[] </allDifferent>
    </constraints>
</instance>
